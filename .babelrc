{
    "env": {
        "production": {
            "plugins": [
                "transform-es2015-modules-amd"
            ]
        },
        "development": {
            "plugins": [
                "transform-es2015-modules-amd"
            ],
            "sourceMaps": "inline"
        },
        "testing": {
            "sourceMaps": "inline"
        }
    },
    "presets": [
        "airbnb",
        "react",
        "es2015",
        "stage-0"
    ],
    "ignore": [
        "dist",
        "node_modules"
    ]
}